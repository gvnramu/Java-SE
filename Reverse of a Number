// Write a function to get reverse of given number
import java.util.Scanner;

public class ReverseNumber {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int rev=0;
		while(n!=0) {
			rev *= 10;
			rev += n%10;
			
			n /= 10;
		}
		System.out.println(rev);
	}

}
