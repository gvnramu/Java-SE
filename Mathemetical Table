import java.util.Scanner;

public class MathsTable {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		printTable(n);
	}
	public static void printTable(int n) {
		for(int i=1;i<=10;i++) {
			System.out.println(n + " * "+ i + " = "+ i*n);
		}
	}
}
